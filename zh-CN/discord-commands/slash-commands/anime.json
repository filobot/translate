{
  "description": "动漫实用工具。",
  "subcommands": {
    "avatar": {
      "description": "随机发送一张动漫头像的图片。"
    },
    "search": {
      "description": "搜索一部动漫的信息。",
      "arguments": {
        "title": {
          "name": "职称",
          "description": "动画片的标题。"
        }
      }
    },
    "random": {
      "description": "根据您指定的标准显示随机动漫中的信息。",
      "arguments": {
        "term": {
          "name": "术语",
          "description": "必须包括动画片标题的术语。"
        },
        "type": {
          "name": "类型",
          "description": "动漫类型。"
        },
        "status": {
          "name": "身份",
          "description": "动漫的地位。"
        },
        "rating": {
          "name": "评级",
          "description": "动漫的评级。"
        },
        "genre": {
          "name": "体裁",
          "description": "体裁或动漫的体裁之一。"
        }
      }
    }
  },
  "subcommand_groups": {
    "characters": {
      "description": "随机发送一张动漫人物的图片或 gif。",
      "subcommands": {
        "ghoul": {
          "description": "随机发送一张食尸鬼的图片。"
        },
        "jojo": {
          "description": "随机发送一张乔乔的图片。"
        },
        "titan": {
          "description": "随机发送一张泰坦的图片。"
        }
      }
    }
  }
}
