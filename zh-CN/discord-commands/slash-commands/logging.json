{
  "description": "允许将服务器的日志发送到一个频道的模块。",
  "subcommands": {
    "enable": {
      "description": "启用日志模块。"
    },
    "disable": {
      "description": "禁用日志模块。"
    },
    "events": {
      "description": "管理你想显示的不同事件。",
      "arguments": {
        "action": {
          "name": "行动",
          "description": "启用或禁用一个事件。"
        }
      }
    },
    "channel": {
      "description": "设置记录通道。",
      "arguments": {
        "channel": {
          "name": "渠道",
          "description": "日志将被发送的通道。"
        }
      }
    },
    "docs": {
      "description": "获取日志模块文档的链接。"
    },
    "view": {
      "description": "查看日志模块的设置。"
    },
    "reset": {
      "description": "将模块设置恢复到原始设置（数据不能恢复）。",
      "arguments": {
        "confirmation": {
          "name": "确认",
          "description": "您真的确定要重置该模块的数据吗？"
        }
      }
    }
  }
}