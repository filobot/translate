{
  "description": "欢迎模块，允许你向新成员发送消息，并为他们分配一个角色。",
  "subcommands": {
    "enable": {
      "description": "启用欢迎模块。",
      "arguments": {
        "module": {
          "name": "模块",
          "description": "你想启用的模块。"
        }
      }
    },
    "disable": {
      "description": "禁用欢迎模块。",
      "arguments": {
        "module": {
          "name": "模块",
          "description": "你想禁用的模块。"
        }
      }
    },
    "channel": {
      "description": "设置发送欢迎信息的通道。",
      "arguments": {
        "channel": {
          "name": "渠道",
          "description": "将发送欢迎信息的渠道。"
        }
      }
    },
    "message": {
      "description": "设置欢迎词。"
    },
    "type": {
      "description": "设置欢迎信息的发送方式。",
      "arguments": {
        "type": {
          "name": "类型",
          "description": "发送欢迎信息的方式。"
        }
      }
    },
    "role": {
      "description": "设置你想授予新成员的角色。",
      "arguments": {
        "role": {
          "name": "作用",
          "description": "你想授予新成员的角色。"
        }
      }
    },
    "docs": {
      "description": "获取欢迎模块文档的链接。"
    },
    "view": {
      "description": "查看欢迎模块的设置。"
    },
    "reset": {
      "description": "将模块设置恢复到原始设置（数据不能恢复）。",
      "arguments": {
        "confirmation": {
          "name": "确认",
          "description": "您真的确定要重置该模块的数据吗？"
        }
      }
    }
  }
}